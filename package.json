  "description": "",
  "main": "index.js",
  "scripts": {
    "start-dev": "concurrently \"npm run watch-client\" \"npm run watch-server\"",
    "watch-client": "webpack --config webpack.dev.config.js --watch",
    "watch-server": "nodemon --watch webpack.server.config.js --watch ./src/server ./src/server/server.js",
    "build-dev": "rm -rf dist && webpack --config webpack.server.config.js && webpack --config webpack.dev.config.js",
    "build-prod": "rm -rf dist && webpack --mode production --config webpack.server.config.js",
    "knex": "knex"
